package com.cdjosecabrera.vista;

public class vista {

	public static void main(String[] args) {
		// TODO Apéndice de método generado automáticamente
		System.out.println("Practicando las tablas de multiplicar");
		for(int y=1;y<11;y++){
		for(int x=1;x<11;x++){
		System.out.println(y+" X "+x+" = "+x*y);
		}}
	}

}
